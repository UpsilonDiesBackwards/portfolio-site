body {
	margin: 0;
	padding: 0;
}
#canvas {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	opacity: 15%;
	z-index: -1;

	background-image: radial-gradient(#22272d 60%, #434a52 80%, #697478 100%);
}

ul {
    list-style: none;
    padding-left: 0;
}

.showcase-image {
	border: 1px solid white;
	transition: transform 0.2s ease;

	width: 300px;
	height: 300px;
}

.showcase-image:hover {
	transform: scale(1.01);
}
