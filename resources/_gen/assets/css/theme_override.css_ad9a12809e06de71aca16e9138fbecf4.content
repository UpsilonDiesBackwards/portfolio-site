body {
	margin: 0;
	padding: 0;
}

ul {
    list-style: none;
    padding-left: 0;
}


.showcase-image:hover {
	transform: scale(1.01);
}

.showcase-element {
	flex: 1; 
	margin-right: 20px;
	align-self: center;
}

.showcase-container {
	display: flex;
}

@media (max-width: 620px) {
	.showcase-container{
		flex-direction: column;
	}
	.footer {
		position: relative;
		top: 0px;
	}

	.showcase-element {
		flex: 1; 
		margin-right: 0px; 
		align-self: center;
	}
}
